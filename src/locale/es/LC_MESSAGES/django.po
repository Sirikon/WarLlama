msgid "Updated: Activity "
msgstr "Actualizado: Actividad "

#: .\Alpaca\emails.py:291
#, python-brace-format
msgid ""
"Hi, {attendant}!{newline}You are receiving this message because the activity "
"{title} was updated!{newline}Check it out at {activity_link}."
"{email_signature}"
msgstr ""
"¡Hola, {attendant}!{newline}¡Has recibido este mensaje porque la actividad "
"{title} ha sido actualizada!{newline}Comprueba los cambios en "
"{activity_link}.{email_signature}"

#: .\Alpaca\emails.py:319
msgid "New Session in Activity "
msgstr "Nueva Sesión en la Actividad "

#: .\Alpaca\emails.py:320
#, python-brace-format
